@section Sidebar {
  @{
    IDocument sidebarRoot = Context.Documents[Docs.Pages].First();
    IReadOnlyList<IDocument> sidebarChildren = sidebarRoot.DocumentList(Keys.Children);
    if(sidebarChildren != null && sidebarChildren.Count > 0)
    {
      @Html.Partial("Sidebar/_ChildPagesMenu", sidebarChildren)
    }
  }
}